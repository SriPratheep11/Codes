import java.util.*;
class Fibo
{  
public static void main(String args[])  
{    
 int a=0,b=1,c,i; 
 Scanner sc=new Scanner(System.in);
 System.out.println("Enter the value of n:\n");
 int n=sc.nextInt();
 System.out.print(a+" "+b);
 for(i=2;i<n;++i)    
  {    
  c=a+b;    
  System.out.print(" "+c);    
  a=b;
  b=c;
  }    
 }
}  
